where coti."NUM_COTIZACION" = dtc."FK_COTIZACION" AND coti."FK_CLIENTE" = cl."ID_CLIENTE" AND e."ID_EMPLEADO" = coti."FK_EMPLEADO"
AND coti."FK_REVISION_ENSAYO_EQUIPO" = rv."ID_REVISION_EQUIPO" AND us.id_usuario = coti."FK_USUARIO" AND coti."US_MODIFICA" = us.id_usuario AND coti."NUM_COTIZACION" = $P{ncotic} AND dtc."FK_COD_TIPO_EQUIPO" = eq."ID_TIPO_EQUIPO" AND eq."FK_UNIDAD" = un."ID_UNIDAD" AND coti."NUM_COTIZACION" = $P{ncotic}]]>
	</queryString>
